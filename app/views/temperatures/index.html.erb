<h1>Listing temperatures</h1>

<%= link_to 'New temperature', new_temperature_path %>

<table>
  <tr>
    <th>Sensor</th>
    <th>From</th>
    <th>To</th>
    <th>Value</th>
    <th></th>
  </tr>

<% @temperatures.each do |temperature| %>
  <tr>
    <td><%= temperature.sensor.Name %></td>
    <td><%= temperature.fromDttm %></td>
    <td><%= temperature.toDttm %></td>
    <td><%= temperature.value %></td>
    <td><%= link_to 'Show', temperature_path(temperature) %></td>
  </tr>
<% end %>
</table>
